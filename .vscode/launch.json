    "version": "0.2.0",
    "configurations": [{
        "name": "Generate rin meta",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}\\meta\\main.py",
        "console": "integratedTerminal",
        "args": [
            "-s",
            "${workspaceFolder}\\include\\rin",
            "-o",
            "${workspaceFolder}\\include\\gen",
            "-w"
        ]
    },
    {
        "name": "Generate standalone meta",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}\\meta\\main.py",
        "console": "integratedTerminal",
        "args": [
            "-s",
            "${workspaceFolder}\\standalone\\src",
            "-o",
            "${workspaceFolder}\\standalone\\src",
            "-w"
        ]
    }]
}